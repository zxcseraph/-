ping -w 1 -c 1 $IP >/dev/null 2>&1
if [ $? != 0 ]; then
   echo "`date +"%H:%M:%S"`: ERROR1: SFTP IP is wrong\n"
   echo "`date +"%H:%M:%S"`: ERROR1: SFTP IP is wrong\n" >>$LOGNE
   exit 1
fi

